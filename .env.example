# Environment Variables for Financi MCP Server

# Azure Function App Settings
FUNCTIONS_WORKER_RUNTIME=python
PYTHONPATH=/home/site/wwwroot
WEBSITE_RUN_FROM_PACKAGE=1

# MCP Server Configuration
MCP_SERVER_NAME=financi
MCP_SERVER_VERSION=1.0.0

# Financial Data API Keys (Add your actual keys here)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Azure Services (These will be automatically configured by Bicep template)
# AzureWebJobsStorage=DefaultEndpointsProtocol=https;AccountName=...
# APPINSIGHTS_INSTRUMENTATIONKEY=...

# Optional: Additional API Keys for enhanced functionality
# FINANCIAL_MODELING_PREP_API_KEY=your_fmp_api_key_here
# IEX_CLOUD_API_KEY=your_iex_api_key_here
# POLYGON_API_KEY=your_polygon_api_key_here

# Logging Level
LOGGING_LEVEL=INFO